{
  "name": "es6-webpack2-starter",
  "version": "2.0.2",
  "description": "A template project for es6/7, webpack2, sass and postcss",
  "scripts": {
    "postinstall": "cp env.example.js env.js",
    "start": "webpack --config webpack/dev.config.js --progress --display-error-details --color --watch",
    "build": "NODE_ENV=production webpack --config webpack/prod.config.js --progress --display-error-details --color",
    "precommit": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micooz/es6-webpack-starter.git"
  },
  "keywords": [
    "es6",
    "es7",
    "webpack2",
    "starter",
    "template",
    "postcss",
    "sass",
    "css module"
  ],
  "author": "Micooz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micooz/es6-webpack-starter/issues"
  },
  "homepage": "https://github.com/micooz/es6-webpack-starter#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.0",
    "open-browser-webpack-plugin": "0.0.4",
    "opn": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-merge": "^4.0.0"
  }
}
